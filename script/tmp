curl -XPUT "localhost:9200/chengpohi/bookmark/_mapping" -d '
{
  "chengpohi" : {
    "mappings" : {
      "bookmark" : {
        "properties" : {
          "_tab_id" : {
            "type" : "string"
          },
          "created_at" : {
            "type" : "date",
            "format" : "dateOptionalTime"
          },
          "name" : {
            "type" : "string"
          },
          "url" : {
            "type" : "string"
          }
        }
      }
    }
  }
}'
